{
  "name": "euro2020",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build-without-db": "blitz build",
    "build": "yarn cache clean && yarn --frozen-lockfile --prod=false --verbose && blitz prisma generate && blitz build && blitz db seed && blitz prisma migrate deploy",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.17",
    "@blitzjs/next": "2.0.0-beta.17",
    "@blitzjs/rpc": "2.0.0-beta.17",
    "@chakra-ui/icons": "2.0.12",
    "@chakra-ui/react": "2.4.1",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@popperjs/core": "2.9.2",
    "@prisma/client": "4.6.0",
    "@sentry/browser": "6.2.5",
    "@sentry/integrations": "6.2.5",
    "@sentry/node": "6.2.5",
    "@sentry/webpack-plugin": "1.15.0",
    "@zeit/next-source-maps": "0.0.3",
    "blitz": "2.0.0-beta.17",
    "dayjs": "1.10.4",
    "eventemitter3": "4.0.7",
    "final-form": "4.20.2",
    "framer-motion": "7.6.7",
    "i18next": "20.2.1",
    "next": "13.0.3",
    "node-fetch": "2",
    "nodemailer": "6.6.1",
    "passport": "0.4.1",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "prisma": "4.6.0",
    "react": "18.2.0",
    "react-datepicker": "3.7.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.3",
    "react-ga": "3.3.0",
    "react-i18next": "11.8.13",
    "react-icons": "4.2.0",
    "react-modal": "3.13.1",
    "react-popper": "2.2.5",
    "ts-node": "10.9.1",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.2.3",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.35",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "eslint": "8.27.0",
    "eslint-config-blitz": "0.45.5",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.7.1",
    "prettier-plugin-prisma": "3.5.0",
    "pretty-quick": "3.1.2",
    "preview-email": "3.0.5",
    "ts-jest": "29.0.3",
    "typescript": "^4.8.4"
  },
  "private": true,
  "engines": {
    "node": ">=0.10.3 <15"
  }
}
